{
  "name": "Copy Cookie Clipboard",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Copy cookie value to clipboard from the context menu.",
  "permissions": [
    "contextMenus",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"],
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": true
  }
}
